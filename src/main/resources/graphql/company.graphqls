type Company {
    id: ID!,
    name: String,
    address: String,
    user: User
}

input CompanyInput {
    name: String,
    address: String,
    user: UserInput
}

type Query {
    getCompanies: [Company],
    getCompany(id: ID!): Company
}

type Mutation {
    createCompany(company: CompanyInput): [Company],
    updateCompany(id: ID!, company: CompanyInput): Company,
    deleteCompany(id: ID!): [Company],
    deleteCompanies: [Company]
}